			    </label>
            </section>
			  				 
	  
	    
	        			  			  <section class="col col-4">
              <label class="label">@Html.DisplayNameFor(model => model.IsBlaclkList) </label>
              <label class="checkbox">
                 <input id="@Html.IdFor(model => model.IsBlaclkList)"
                       name="@Html.NameFor(model => model.IsBlaclkList)"
                       value="true" @(Model.IsBlaclkList ? "checked=\"checked\"" : "")
                       tabindex="6"
                       type="checkbox" />
                <i></i>
                <b class="tooltip tooltip-bottom-right">黑名单</b>
            </label>
            </section>
              				 
	  
	    
	        			  			  <section class="col col-4">
              <label class="label">@Html.DisplayNameFor(model => model.IsDeleteFlag) </label>
