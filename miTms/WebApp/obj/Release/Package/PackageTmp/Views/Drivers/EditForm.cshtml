			    </label>
            </section>
			  				 
	  
	    
	                     <section class="col col-4">
            <label class="label">@Html.DisplayNameFor(model => model.Carrierid) </label>
            <label class="input">
			      @Html.DropDownListFor(model => model.Carrierid, ViewBag.Carrierid as List<SelectListItem>, htmlAttributes: new { @class = "select2" }) 
                 <b class="tooltip tooltip-bottom-right">承运人</b>
			    </label>
            </section>
				 
	  
	    
	        			              <section class="col col-4">
            <label class="label">@Html.DisplayNameFor(model => model.RegistrationDatetime) </label>
            <label class="input">
            <i class="icon-append fa fa-calendar"></i>
			<input id="@Html.IdFor(model => model.RegistrationDatetime)" 
			       name="@Html.NameFor(model => model.RegistrationDatetime)" 
                   placeholder="@Html.DisplayNameFor(model => model.RegistrationDatetime)" 
				   tabindex="9" 
				   class="datepicker" data-dateformat="dd/mm/yy"
				   type="text" 
				   value="@Html.ValueFor(model => model.RegistrationDatetime)"
				    />
             <b class="tooltip tooltip-bottom-right">注册日期</b>
			    </label>
            </section>
			  				 
	  
	    
