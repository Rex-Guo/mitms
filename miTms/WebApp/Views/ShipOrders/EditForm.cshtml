	function shiporderdetails_saveitem() {
			if (shiporderdetails_endEditing()) {
				if ($shiporderdetailsdg.datagrid("getChanges").length) {
					var inserted = $shiporderdetailsdg.datagrid("getChanges", "inserted");
					var deleted = $shiporderdetailsdg.datagrid("getChanges", "deleted");
					var updated = $shiporderdetailsdg.datagrid("getChanges", "updated");
					var effectRow = new Object();
					if (inserted.length) {
						effectRow.inserted = inserted;
					}
					if (deleted.length) {
						effectRow.deleted = deleted;
					}
					if (updated.length) {
						effectRow.updated = updated;
					}
					//console.log(JSON.stringify(effectRow));
					$.post("/ShipOrderDetails/SaveData", effectRow, function (response) {
						//console.log(response);
						if (response.success) {
							$.messager.alert("提示", "提交成功！");
							$shiporderdetailsdg.datagrid("acceptChanges");
							$shiporderdetailsdg.datagrid("reload");
						}
					}, "json").fail(function (response) {
