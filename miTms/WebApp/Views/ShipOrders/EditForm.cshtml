								<li class="active"><a data-toggle="tab" href="#tab-shiporderdetails"> ShipOrderDetails</a></li>
				   
		 </ul>
		 <!-- Tab panes -->
         <div class="tab-content">
		 		  <div id="tab-shiporderdetails" class="tab-pane active">
            <div class="panel-body">
                <div class="table-responsive">
				<table id="shiporderdetails_datagrid" >
                </table>
				<div id="shiporderdetails_toolbar" style="height:auto">
                    <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'fa fa-plus fa-fw',plain:true" onclick="shiporderdetails_createitem()">新增</a>
                    <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'fa fa-trash-o fa-fw',plain:true" onclick="shiporderdetails_removeitem()">删除</a>
                    <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'fa fa-floppy-o fa-fw',plain:true" onclick="shiporderdetails_saveitem()">保存</a>
                    <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'fa fa-undo fa-fw',plain:true" onclick="shiporderdetails_reject()">取消</a>
                </div>
                </div>
            </div>
        </div>
		  	</div>
	</div>
 
<!--end details tabs -->
        </section>
    </div>
</fieldset>



 <script type="text/javascript">
   var id=@Model.Id;
       $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
        var target = $(e.target).attr("href") // activated tab
                if(target=="#tab-shiporderdetails")
             $shiporderdetailsdg.datagrid("resize");
            });
		 
								//--------------------------------------------------------------------
				// #shiporderdetails_datagrid functions
				//--------------------------------------------------------------------
				var $shiporderdetailsdg = $("#shiporderdetails_datagrid");

				var shiporderdetails_editIndex = undefined;
		function shiporderdetails_reload() {
				$shiporderdetailsdg.datagrid("reload");
			}
	function shiporderdetails_endEditing() {
		if ( shiporderdetails_editIndex == undefined) { return true }
		if ($shiporderdetailsdg.datagrid("validateRow", shiporderdetails_editIndex)) {
						var ed = $shiporderdetailsdg.datagrid("getEditor", { index: shiporderdetails_editIndex, field: "OrderId" });
			var OrderOrderNo = $(ed.target).combobox("getText");
			var OrderId = $(ed.target).combobox("getValue");
		  
			$shiporderdetailsdg.datagrid("getRows")[shiporderdetails_editIndex]["OrderOrderNo"] = OrderOrderNo;
			$shiporderdetailsdg.datagrid("getRows")[shiporderdetails_editIndex]["OrderId"] = OrderId;
			
			
						$shiporderdetailsdg.datagrid("endEdit", shiporderdetails_editIndex);
			shiporderdetails_editIndex = undefined;


			return true;
		} else {
			return false;
		}
	}
	function shiporderdetails_onClickCell(index, field) {
		if (shiporderdetails_editIndex != index) {
			if (shiporderdetails_endEditing()) {
				$shiporderdetailsdg.datagrid("selectRow", index)
						.datagrid("beginEdit", index);
                shiporderdetails_editIndex  = index;
				var ed = $shiporderdetailsdg.datagrid("getEditor", { index: index, field: field });
				if (ed) {
